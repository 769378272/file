--2.1.4.11 YWS_HOS_EMPLOYEE_POSITION  医院员工职称字典表
CREATE OR REPLACE VIEW YWS_HOS_EMPLOYEE_POSITION AS
select NULL IS_OP_IP	    ,--门诊住院标识
      T.TITLE POSITION_CODE	,--职称代码
      (SELECT VALUE_NAME FROM PUBLIC_DICT WHERE DICT_NAME = 'TITLE_DICT' AND T.TITLE = VALUE_ID) POSITION_NAME	,--职称名称
      NULL POSITION_TYPE	,--职称类型
      NULL HOS_CODE	    --医院代码
       from USER_INFO T