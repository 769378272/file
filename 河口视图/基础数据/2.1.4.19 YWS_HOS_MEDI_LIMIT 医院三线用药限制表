CREATE OR REPLACE VIEW YWS_HOS_MEDI_LIMIT AS
--2.1.4.19 YWS_HOS_MEDI_LIMIT 医院三线用药限制表
select 
  NULL IS_OP_IP	 , --门诊、住院标记
  T2.DEPT_CODE LIMIT_KESHI, --科室编码
  T.USER_ID LIMIT_DOC	 , --医生工号
  T.TITLE LIMIT_POSIT, --医生级别(职称)代码
  NULL LIMIT_LEVEL, --限制级别
  NULL HOS_CODE	    --医院代码
FROM USER_INFO T left join USER_DEPT_REL T2 ON T.USER_ID = T2.USER_ID